<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<!-- namespace속성 : 아래에서 작성한 SQL을 호출할 때 같이 사용된다. -->			
<sqlMap namespace="qna">
	
	 
	 
	 <select id="selectListAll" resultClass="qnaVO">
	 		select qna_no, qna_indate, qna_title,qna_view_cnt from qna
	 </select>
	 
	 
	 <select id="qnaDetailContent" resultClass="qnaVO" parameterClass="String">
         
		  select qna_type, qna_title, qna_indate, qna_content from qna where qna_no = #String#
      </select>
	 
	 
	 
	<!-- 문의사항 글 등록 insert ////////////////
	<insert id="insertSinger" parameterClass="singerVO">
		<selectKey resultClass="int" keyProperty="id">
		select SEQ_SING_NO.nextval as id
		from dual
		</selectKey>
	insert into singer (SING_NO,
						SING_NAME,
						SING_NATION,
						SING_ACT_TYPE,
						SING_DEBUT_MUS,
						SING_ACT_ERA,
						SING_IMAGE,
						SING_DEBUT_ERA,
						SING_INTRO)
				values (#id#,
						#sing_name#,
						#sing_nation#,
						#sing_act_type#,
						#sing_debut_mus#,
						#sing_act_era#,
						#sing_image#,
						#sing_debut_era#,
						#sing_intro#)		
	</insert> -->
		   
		
	
	

</sqlMap>