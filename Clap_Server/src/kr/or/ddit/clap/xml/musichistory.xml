<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
			PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
			"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<!-- namespace속성 : 아래에서 작성한 SQL을 호출할 때 같이 사용된다. -->			
<sqlMap namespace="musichistory">
	
	 
	 <select id="selectList" resultClass="musichistoryVO">
	 		select mus_no ,count(mus_no)
			  from music_history
			 where histo_indate = to_char(sysdate,'YYYYMMDD')
			 group by mus_no
			 order by count(mus_no) desc
	 </select>
	   
	  
		
	
	

</sqlMap>			





